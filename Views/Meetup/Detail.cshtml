Page Content -->
<div class="container">

    <!-- Portfolio Item Heading -->
    <h1 class="my-4">@Model.Title</h1>
    <h3>@Model.Address.City, @Model.Address.State</h3>
  
  
    <!-- Portfolio Item Row -->
    <div class="row">
  
      <div class="col-md-8">
        <img class="img-fluid" src=@Model.Image alt="">
      </div>
  
      <div class="col-md-4">
        <h3 class="my-3">@Model.Title</h3>
        <p>@Model.Description</p>
        <h3 class="my-3">@Model.MeetupCategory</h3>
        <ul>
          <li>@Model.StartTime</li>
          <li>@Model.Address.Street</li>
          <li>@Model.Address.City, @Model.Address.State</li>
          <li>@Model.MeetupCategory</li>
        </ul>
        <h3 class="my-3">Contact</h3>
        <p class="my-3">@Model.Email</p>
      </div>
  
    </div> 
</div>